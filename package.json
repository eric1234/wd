{
  "name": "whatcha-doin",
  "genericName": "Time Tracker",
  "version": "0.0.1",
  "description": "Keeps track of what your doing by asking you",
  "main": "src/boot.js",
  "author": {
    "name": "Eric Anderson",
    "email": "eric@pixelwareinc.com",
    "url": "https://github.com/eric1234/wd"
  },
  "license": "Unlicense",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "electron-prebuilt-compile": "1.6.8",
    "timekeeper": "^1.0.0"
  },
  "dependencies": {
    "electron-compile": "^6.4.1",
    "nedb": "^1.8.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "scripts": {
    "start": "DB=events.db electron-forge start",
    "test": "ava",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["zip"],
        "linux": ["deb", "rpm"]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {},
      "electronInstallerDebian": {
        "icon": "src/icon.png"
      },
      "electronInstallerRedhat": {
        "icon": "src/icon.png"
      },
      "github_repository": {
        "owner": "eric1234",
        "name": "wd"
      },
      "windowsStoreConfig": {}
    }
  }
}
